{
  "name": "nanoevents",
  "version": "1.0.2",
  "description": "Simple and tiny (120 bytes) event emitter library",
  "keywords": [
    "EventEmitter",
    "Events",
    "emit",
    "emitter",
    "event",
    "pub/sub",
    "publish",
    "reactor",
    "subscribe"
  ],
  "author": "Andrey Sitnik <andrey@sitnik.ru>",
  "license": "MIT",
  "repository": "ai/nanoevents",
  "dependencies": {},
  "devDependencies": {
    "docdash": "^0.4.0",
    "eslint": "^4.18.2",
    "eslint-ci": "^0.1.1",
    "eslint-config-logux": "^21.0.0",
    "eslint-config-standard": "^11.0.0",
    "eslint-plugin-es5": "^1.2.0",
    "eslint-plugin-import": "^2.9.0",
    "eslint-plugin-jest": "^21.13.0",
    "eslint-plugin-node": "^6.0.1",
    "eslint-plugin-promise": "^3.6.0",
    "eslint-plugin-security": "^1.4.0",
    "eslint-plugin-standard": "^3.0.1",
    "jest": "^22.4.2",
    "jsdoc": "^3.5.5",
    "lint-staged": "^7.0.0",
    "pre-commit": "^1.2.2",
    "rimraf": "^2.6.2",
    "size-limit": "^0.16.1",
    "yaspeller-ci": "^1.0.0"
  },
  "scripts": {
    "lint-staged": "lint-staged",
    "spellcheck": "yarn docs && yaspeller-ci *.md docs/*.html",
    "clean": "rimraf docs/ coverage/",
    "lint": "eslint-ci *.js test/*.js",
    "docs": "jsdoc --configure .jsdocrc *.js",
    "test": "jest --coverage && yarn lint && size-limit && yarn spellcheck"
  },
  "jest": {
    "testEnvironment": "node",
    "coverageThreshold": {
      "global": {
        "statements": 100
      }
    }
  },
  "engines": {
    "node": ">=4.0.0"
  },
  "pre-commit": [
    "lint-staged"
  ]
}
